<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
<style name="main_radio_botton">
        <item name="android:gravity">bottom</item>
        <item name="android:button">@null</item>
         <item name="android:padding">0dp</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="webview_bar_style">
        <item name="android:scrollbarThumbVertical">@drawable/weview_bar</item>
        <item name="android:scrollbarSize">5dp</item>
    </style>
     <style name="title_bar">
    <item name="android:textColor">@color/whilt</item>
    <item name="android:gravity">center</item>
    <item name="android:textSize">16sp</item>
     <item name="android:textStyle">bold</item>
    </style>
    <style name="dialog" parent="@android:style/Theme.Dialog">
         <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/photo_shape</item>
        <item name="android:backgroundDimEnabled">true</item>
       
        
    </style>
     
</resources>